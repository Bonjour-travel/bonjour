// 将设置放入此文件中以覆盖默认设置
{
  "editor.fontSize": 14,
  "java.errors.incompleteClasspath.severity": "ignore",
  "sublimeTextKeymap.promptV3Features": true,
  "eslint.enable": true,
  "editor.wrappingIndent": "same",
  //微信小程序
  "files.associations": {
    "*.wxml": "html",
    "*.wxss": "css",
    "*.wpy": "vue"
  },
  "workbench.startupEditor": "welcomePage",
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "editor.formatOnSave": true
}